        Quantum`Notation`zz080KetArgs,
        BaseStyle->{ShowSyntaxStyles -> True},
        Editable->True,
        Selectable->True], "\[RightAngleBracket]"}],
      Quantum`Notation`zz080Ket,
      BaseStyle->{ShowSyntaxStyles -> False},
      Editable->False,
      Selectable->False], "+", 
     TagBox[
      RowBox[{"\[VerticalSeparator]", 
       TagBox[
        SubscriptBox["1", 
         OverscriptBox["1", "^"]],
        Quantum`Notation`zz080KetArgs,
